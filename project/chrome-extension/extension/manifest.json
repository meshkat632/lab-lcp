{
  "name": "lcp chrome extension",
  "description": "",
  "version": "2.0",
  "icons": { "16": "assets/research-icon.png", "48": "assets/research-icon.png", "128": "assets/research-icon.png" },
  "permissions": [
    "activeTab","tabs","background"
  ],
  "background": {
    //"scripts": ["background/background.js"],
    "page": "background/background.html",
    "persistent": true
  },
  "options_page": "options/options-new.html",
  "browser_action": {
    "default_title": "rating page",
    "default_icon": "assets/research-icon.png",
    "default_popup": "home/home.html"
  },
  "permissions":["tabs","<all_urls>"],
  "web_accessible_resources": ["options/options.html", "options/*"],
  "manifest_version": 2
}